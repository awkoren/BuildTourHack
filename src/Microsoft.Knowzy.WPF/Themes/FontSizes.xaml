<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:Double x:Key="FontSizeExtraMicro">10</system:Double>
    <system:Double x:Key="FontSizeMicro">12</system:Double>
    <system:Double x:Key="FontSizeExtraSmall">13</system:Double>
    <system:Double x:Key="FontSizeSmall">14</system:Double>
    <system:Double x:Key="FontSizeMedium">16</system:Double>
    <system:Double x:Key="FontSizeSemiLarge">18</system:Double>
    <system:Double x:Key="FontSizeLarge">26</system:Double>
    <system:Double x:Key="FontSizeMoreLarge">52</system:Double>

</ResourceDictionary>
